#include "mdm9640-v2.dtsi"
#include "mdm9640-pinctrl.dtsi"

&soc {
	sound {
		qcom,cdc-micbias2-headset-only;
	};
};

&blsp1_uart3 {
	status = "ok";
	pinctrl-names = "default";
	pinctrl-0 = <&uart3_console_active>;
};

&blsp1_uart2 {
	status = "ok";
};

&ipa_hw {
	qcom,ipa-hw-mode = <1>; /* IPA hw type = Virtual */
};

&qnand_1 {
	status = "ok";
};

